    "pkce",
    "session",
    "redis",
    "postgresql",
    "typescript"
  ],
  "author": {
    "name": "FrankFMY",
    "email": "Pryanishnikovartem@gmail.com",
    "url": "https://github.com/FrankFMY"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/FrankFMY/auth-bff-oidc-template.git"
  },
  "bugs": {
    "url": "https://github.com/FrankFMY/auth-bff-oidc-template/issues"
  },
  "homepage": "https://github.com/FrankFMY/auth-bff-oidc-template#readme",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
